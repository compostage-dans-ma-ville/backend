<mjml>
  <mj-head>
    <mj-title>{{ title }}</mj-title>
    <mj-attributes>
      <mj-all font-family="Montserrat, Helvetica, Arial, sans-serif"></mj-all>
      <mj-text font-weight="400" font-size="16px" color="#000000" line-height="24px"></mj-text>
      <mj-section padding="0px"></mj-section>
      <mj-button name="button" background-color="#0A6259" color="#fff" font-size="18px" border-radius="8px"></mj-button>
    </mj-attributes>
  </mj-head>
  <mj-body background-color="#FEFFF6">
    {{> header }}
    
    {{> @partial-block}}

    {{> footer }}
  </mj-body>
</mjml>