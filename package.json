{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:types": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "prisma:generate": "prisma generate",
    "db:inspect": "prisma studio",
    "db:start": "docker run --name compostage_dev_db --env-file .env -p 5432:5432 -v compostage_dev_db:/var/lib/postgresql/data -d postgres:15.1",
    "db:stop": "docker container stop compostage_dev_db && docker container rm compostage_dev_db",
    "db:init": "yarn prisma migrate reset",
    "db:sh": "psql -h localhost -p 5432 -U admin compostage_dev",
    "seed:fake": "ts-node src/seeder",
    "seed:fake:refresh": "ts-node src/seeder --refresh",
    "seed:real": "prisma migrate reset --force && ts-node src/seeder/data/ActivateurSeeder.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.3.3",
    "@casl/prisma": "^1.3.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "bcrypt": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "mjml": "^4.14.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^4.10.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.0",
    "transform-coordinates": "^1.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/testing": "^9.0.0",
    "@prisma/client": "^4.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "29.2.4",
    "@types/mjml": "^4.7.1",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/proj4": "^2.5.2",
    "@types/supertest": "^2.0.11",
    "@types/transform-coordinates": "^1.0.0",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "nestjs-seeder": "^0.3.0",
    "prisma-dbml-generator": "^0.10.0",
    "proj4": "^2.9.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "validatorjs": "^3.22.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^~/(.*)": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "~/coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.1"
}
